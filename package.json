{
  "name": "ci-report",
  "version": "0.0.1",
  "description": "A web application to display tests reports for CI tools like Travis or Gitlab-ci",
  "devDependencies": {
    "@symfony/webpack-encore": "^0.9.1",
    "bootstrap": "4.0.0-alpha.6",
    "font-awesome": "^4.7.0",
    "imports-loader": "^0.7.1",
    "lato-font": "^3.0.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5"
  },
  "scripts": {
    "dev-server": "./node_modules/.bin/encore dev-server",
