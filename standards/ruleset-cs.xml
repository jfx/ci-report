<?xml version="1.0"?>
<ruleset name="ci-report CS">
    <description>ci-report coding standard</description>

    <rule ref="../vendor/escapestudios/symfony2-coding-standard/Symfony/">

    </rule>
    
    <!-- Use Unix newlines -->
    <rule ref="Generic.Files.LineEndings">
        <properties>
            <property name="eolChar" value="\n"/>
        </properties>
    </rule>
      
</ruleset>
